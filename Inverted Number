import java.util.Scanner;

public class MyClass {
    public static void main(String args[]) {
     Scanner sc = new Scanner(System.in);
     
     int num = sc.nextInt();
     int op =1;
     int inv =0;
     while(num > 0){
         
         int od = num % 10;
         int  ip = od;
         int  id = op;
         
         inv = inv + id * (int)Math.pow(10, ip -1);
         
         num = num /10;
         op++;
         
     }
    System.out.println(inv);

    }
    
}
