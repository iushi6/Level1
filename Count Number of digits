import java.util.Scanner;

public class MyClass {
    public static void main(String args[]) {
     Scanner sc = new Scanner(System.in);
     
     int num = sc.nextInt();
     int n = num;
     int count =0;
     
     while(n>0){
         n = n/10;
         count++;
         
     }
     
     System.out.println("Number of digits are :" + count);
     
    }
    
}
